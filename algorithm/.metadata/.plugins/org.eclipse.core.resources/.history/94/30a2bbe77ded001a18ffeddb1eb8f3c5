public class basic22 {

	public static void main(String[] args) {
		
		int input  = 123211;
		int count = 0;
		int temp = input;
		boolean result = false;
		
		while(temp!=0) {
			temp = temp/10;
			if(temp!=0) {
				count++;
			}
			
		}
		System.out.println(count);
		
	}
	
	
}

