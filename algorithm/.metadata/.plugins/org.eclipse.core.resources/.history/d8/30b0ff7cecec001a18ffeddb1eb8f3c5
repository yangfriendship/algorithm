package basic100;
//14
//숫자 개수 출력
public class basic14 {

	public static void main(String[] args) {
		
		String input = "421314218";

		char[] list = input.toCharArray();
		
		int[] result = new int[10];
		
		for(char item : list) {
			result[item-'0'] +=1;
		}
		
		for (int i = 0; i < result.length; i++) {
			
			System.out.printf("%d : %d",i,result[i]);
		}
		
		
	}

}
