package basic100;


//ÆÑÅä¸®¾ó
// 5ÆÑÅä¸®¾ó = 5 x 4 x 3 x 2 x 1
public class basic8_recursionFunction {

	
	public static void main(String[] args) {
		
		int num = 5;
		
		int result =num;
		
		for(int i=num-1; i > 0 ; i--) {
			
			result = result* i;
			
		}
		System.out.println(result);
		
	}


}
