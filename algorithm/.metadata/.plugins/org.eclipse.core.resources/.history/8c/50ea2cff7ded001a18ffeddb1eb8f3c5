//12321  return : ture
//12345 return : false
public class basic22 {

	public static void main(String[] args) {

		int input = 1232111;
		int count = 0;
		int temp = input;
		boolean result = false;

		while (temp != 0) {
			if (temp != 0) {
				count++;
			}

			temp = temp / 10;
		}
		System.out.println(count);

	}

}
