package basic100;

public class basic24 {
	
	

	public static float average(int num, int[] scoures) {

		int sum = 0;
		int count = 0;
		for (int scoure : scoures) {
			sum += scoure;
		}
		float average = sum/num;
		
		for (int soucre : scoures) {
			if(soucre > average) {
				count ++;
			}
		}
		
		float result = count/num;
		
		return result;
	}
	
	public static float aboveStudent(int num,float average) {
		
		
		
		return average;
	}

	public static void main(String[] args) {
		int num = 7;
		int[] scoures = { 100, 95, 90, 80, 70, 60, 50 };
		// default input

		System.out.println(average(num,scoures));
		
	}

}
