package basic100;

//°ÅµìÁ¦°ö
public class basic19 {

	public static int power(int num,int num1) {
		int result = 1;
		while(num1!=0) {
			result *= num;
			num1 --;
		}
		
		return result;
	}
	
	
	public static void main(String[] args) {
		int num = 5;
		int num1 = 3;
		
		System.out.println(power(num,num1));
		
	}
	
}
