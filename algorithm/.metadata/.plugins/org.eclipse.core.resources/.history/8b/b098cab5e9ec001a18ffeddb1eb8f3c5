package basic100;
//12
//또 사각형
public class basic12 {

	public static void main(String[] args) {
		
		int input =4;
		int[][] list = new int [input][input];
		int count = input;
		for (int i = 1; i < input+1; i++) {
			
			for (int j = 1; j < list.length+1; j++) {
				
				System.out.printf("%4d",j*i+input);
				
			}
			
		}
		
		
	}
	
}
