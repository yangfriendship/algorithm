package basic100;

public class basic_10 {

	
	public static void main(String[] args) {
		
		int input = 4;
		int count = 0;
		for (int i = 0; i < input; i++) {
			
			for (int j = 1; j < input; j++) {
				
				System.out.print(j+count);
				
			}
			count+=input;
			System.out.println();
		}
		
		
	}
}
