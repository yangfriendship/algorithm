package basic100;

//´ŞÆØÀÌ
public class basic27 {

	public static void main(String[] args) {

		int input = 5;
		int num = 1;
		int x = 0;
		int y = 0;
		int f = input;
		
		int[][] result = new int[input][input];

		for (int i = 0; i < 2 * input - 1; i++) {

			switch (i % 4) {
			case 0: // right
				for (int j = 0; j < f; j++) {
					result[x][y] = num;
					
				}
				break;

			case 1:
				break;
			case 2:
				break;
			case 3:
				break;

			}

		}

	}

}
