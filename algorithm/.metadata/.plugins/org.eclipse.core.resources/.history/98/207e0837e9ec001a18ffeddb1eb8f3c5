package basic100;
//12
//또 사각형
public class basic12 {

	public static void main(String[] args) {
		
		int input =4;
		int[][] list = new int [input][input];
		int count = input;
		for (int i = 0; i < input; i++) {
			
			for (int j = 0; j < list.length; j++) {
				
				System.out.printf("%d",i*(j*input));
			}
			
			
		}
		
		
	}
	
}
