package basic100;

//Æç¸°µå·Ò
//12321  return : ture
//12345 return : false
public class basic22 {

	public static void main(String[] args) {

		int input = 12321;
		int count = 0;
		int temp = input;
		boolean result = false;

		while (temp != 0) {
			if (temp != 0) {
				count++;
			}
			temp = temp / 10;
		}

		int[] list = new int[count];
		for (int i = 0; i < list.length; i++) {
			list[i] = input/10;
		}
		
		for(int item : list) {
			System.out.println(item);
		}

	}

}
