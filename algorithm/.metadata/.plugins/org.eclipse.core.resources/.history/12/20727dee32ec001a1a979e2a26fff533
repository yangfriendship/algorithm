package basic100;

//최대공약수
//input : 12 18
//answer : 6
public class basic6 {
	
	public static int[] divisor(int input) {
		int[] arr = new int[input];
		int count =0;
		for (int i = 0; i < arr.length; i++) {
			if(input%i==0) {
				arr[count] = i;
				count ++;
			}
		}
		
		return arr;
	}

	public static void main(String[] args) {

		int gcf = 0;
		
		int num = 12;
		int num1 = 18;
		
		int[] arr = divisor(num);
		int[] arr1 = new int[num1];
		
		for(int item : arr) {
			System.out.print(item + " ");
		}
		
		
		
		
		
	}//
}
